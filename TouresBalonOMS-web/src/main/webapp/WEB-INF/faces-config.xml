<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <application>
        <resource-bundle>
            <base-name>mensajes.messages</base-name>
            <var>mensajes</var>
        </resource-bundle>        
    </application>
    <application>
        <resource-bundle>
            <base-name>mensajes.productos_msgs</base-name>
            <var>mensajesProductos</var>
        </resource-bundle>        
    </application>
    <application>
        <resource-bundle>
            <base-name>mensajes.campanias_msgs</base-name>
            <var>mensajesCampanias</var>
        </resource-bundle>        
    </application>
    <application>
        <resource-bundle>
            <base-name>mensajes.clientes_msgs</base-name>
            <var>mensajesClientes</var>
        </resource-bundle>        
    </application>
    <application>
        <resource-bundle>
            <base-name>mensajes.ordenes_msgs</base-name>
            <var>mensajesOrdenes</var>
        </resource-bundle>        
    </application>
        
</faces-config>
